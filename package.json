{
  "name": "unocss-preset-wind-extra",
  "type": "module",
  "version": "0.6.1",
  "packageManager": "pnpm@8.7.0",
  "description": "Improve tailwind unocss to reduce length of classes",
  "author": "Jonathan <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter",
    "tailwind"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "unbuild --stub",
    "prepublishOnly": "bun run build",
    "release": "wireit",
    "format": "wireit",
    "bump": "./bump.sh"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@biomejs/biome": "1.3.3",
    "@types/node": "^20.7.0",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "wireit": "^0.14.1"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "biome": "latest",
    "string-ts": "^1.3.2"
  },
  "wireit": {
    "build": {
      "command": "unbuild",
      "files": [
        "uno.config.ts"
      ],
      "dependencies": []
    },
    "release": {
      "command": "pnpm publish --no-git-checks",
      "dependencies": [
        "format",
        "build"
      ]
    },
    "format": {
      "command": "bunx @biomejs/biome format --write ."
    }
  }
}