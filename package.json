{
  "name": "unocss-preset-wind-extra",
  "type": "module",
  "version": "0.9.2",
  "packageManager": "pnpm@8.7.0",
  "description": "Improve tailwind unocss to reduce length of classes",
  "author": "Jonathan <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter",
    "tailwind"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "release": "wireit",
    "format": "wireit",
    "bump": "./bump.sh",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "vite playground",
    "deploy": "vite build playground"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@biomejs/biome": "1.3.3",
    "@types/node": "^20.7.0",
    "@unocss/preset-wind": "^0.57.7",
    "@vitejs/plugin-vue": "^4.3.4",
    "bun-types": "^1.0.14",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.57.7",
    "unocss-preset-wind-extra": "0.7.12",
    "vite": "^4.4.9",
    "vitest": "^0.34.5",
    "wireit": "^0.14.1",
    "bumpp": "latest"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "biome": "latest",
    "string-ts": "latest"
  },
  "wireit": {
    "build": {
      "command": "unbuild",
      "files": [
        "uno.config.ts"
      ]
    },
    "release": {
      "command": "pnpm publish --no-git-checks",
      "clean": true,
      "dependencies": [
        "format",
        "build"
      ],
      "output": [
        "dist"
      ]
    },
    "format": {
      "command": "bun x @biomejs/biome format --write ."
    },
    "dev": {
      "command": "bun --watch run test.ts",
      "clean": true
    }
  }
}
