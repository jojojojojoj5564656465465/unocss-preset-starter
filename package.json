{
  "name": "unocss-preset-starter",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@8.7.0",
  "description": "<description>",
  "author": "unpreset <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "bun run build",
    "release": "pnpm publish --no-git-checks ",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "vite playground",
    "deploy": "vite build playground"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.10",
    "@babel/types": "^7.23.0",
    "@types/node": "^20.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "biome": "^0.3.3",
    "string-ts": "^1.3.2"
  }
}
